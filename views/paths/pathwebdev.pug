extends ../layout

block content
  .container-fluid-wider1
    .col-sm-3.col-xs-6
      h3 Hi #{user.fname}, 
         br
         | Please find your personal learning path in #{user.profile.career}
         
      p Using our machine learning algorithm we have defined a personal learning path adapted to your needs depedning on the information you have provided.  
          

  .container
   br
   .col-sm-4.col-xs-8.col-sm-offset-0.col-xs-offset-2
    .box
     h3#roadmap-titre 	 Skills you will learn:
     p  	 HTML, CSS, Javascript, Angular, Node
     
   .col-sm-4.col-xs-8.col-sm-offset-0.col-xs-offset-2
    .box
     h3#roadmap-titre 	 Job outcomes:
     p  	 Web Developper
   .col-sm-4.col-xs-8.col-sm-offset-0.col-xs-offset-2
    .box
     h3#roadmap-titre 	 Learning time:
     
     //70
     p 
      if user.profile.hours == "five"
        p 3.5 Months
      else if user.profile.hours == "ten"
        p 2 Months
      else if user.profile.hours == "twenty"
        p 1 Months
      else 
        p 2 Weeks

