      .col-md-2.col-sm-2.hidden-xs.box1

        img(src="https://www.edx.org/sites/default/files/styles/course_video_banner/public/course/image/featured-card/softconst1-v4_318x210.jpg?itok=dycRoZS6" style="width:150px;height:106px;")
      .col-md-6.col-sm-8.col-xs-10.box5
        a(target='_blank', href="https://www.edx.org/course/software-construction-data-abstraction-ubcx-softconst1x")
